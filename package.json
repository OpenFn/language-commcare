{
  "name": "@openfn/language-commcare",
  "version": "1.4.0",
  "description": "Commcare Language Pack for OpenFn",
  "homepage": "https://docs.openfn.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/openfn/language-commcare.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "ast": "simple-ast --adaptor ./src/Adaptor.js --output ast.json",
    "build": "node_modules/.bin/babel src -d lib && npm run ast",
    "postversion": "git push && git push --tags",
    "test": "mocha --require @babel/register",
    "test:watch": "mocha -w --require @babel/register",
    "version": "npm run build && git add -A lib ast.json"
  },
  "author": "Open Function Group",
  "license": "LGPLv3",
  "files": [
    "lib/",
    "ast.json"
  ],
  "dependencies": {
    "@openfn/language-common": "1.4.2",
    "@openfn/language-http": "^3.1.11",
    "JSONPath": "^0.10.0",
    "form-data": "^4.0.0",
    "js2xmlparser": "^1.0.0",
    "lodash-fp": "^0.10.2",
    "superagent": "^6.1.0",
    "xlsx": "^0.17.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "@openfn/simple-ast": "^0.3.2",
    "assertion-error": "^1.0.1",
    "chai": "^4.2.0",
    "deep-eql": "^4.0.0",
    "jsdoc": "^3.6.6",
    "mocha": "^7.1.0",
    "nock": "^13.0.5",
    "sinon": "^9.2.3",
    "superagent-mock": "^1.10.0"
  },
  "directories": {
    "lib": "./lib"
  },
  "bundledDependencies": [
    "@openfn/language-common",
    "form-data",
    "js2xmlparser",
    "JSONPath",
    "lodash-fp",
    "superagent",
    "xlsx"
  ]
}
